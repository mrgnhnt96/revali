{
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "all",
    "tabWidth": 4,
    "semi": true,
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "parser": "json"
            }
        },
        {
            "files": ["*.ts", "*.tsx"],
            "options": {
                "parser": "typescript"
            }
        }
    ]
}
